{
  "name": "autosend",
  "version": "1.0.0",
  "description": "Node.js SDK for Autosend with Resend-compatible adapter",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./resend": {
      "import": {
        "types": "./dist/types/adapters/resend/index.d.ts",
        "default": "./dist/esm/adapters/resend/index.mjs"
      },
      "require": {
        "types": "./dist/types/adapters/resend/index.d.ts",
        "default": "./dist/cjs/adapters/resend/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json && node scripts/fix-esm.js",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/fix-cjs.js",
    "build:types": "tsc -p tsconfig.types.json",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && npm test"
  },
  "keywords": [
    "email",
    "autosend",
    "resend",
    "transactional",
    "sdk"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@vitest/coverage-v8": "^2.1.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false
}
